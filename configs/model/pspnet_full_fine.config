# Traing baseline PSPNet model from ResNet 50 weights with 713 crop.

model {
    pspnet {
        num_classes: 19
        filter_scale: 1.0
        scale_predictions: true
        train_reduce: false
        feature_extractor {
            type: 'dilated_resnet50'
        }
        hyperparams {
            batch_norm {
                train: false
                scale: true
                center: true
                decay: 0.9997
                epsilon: 0.00001
            }
            regularizer {
                l2_regularizer {
                     weight: 0.0001
                }
            }
            initializer {
                variance_scaling_initializer {}
            }
        }
        loss {
            classification_loss {
                softmax {}
            }
            dist_loss {
                none {}
            }
            use_auxiliary_loss: true
        }
    }
}

train_config: {
    batch_size: 3
    num_steps: 20000
    fine_tune_checkpoint_type: "segmentation"
    fine_tune_checkpoint: "tune_all_logs/bdd_wd_moo2/model.ckpt-4265"
    #fine_tune_checkpoint: "remote/train_logs/resnet/model.ckpt-8293"
    optimizer {
        momentum_optimizer: {
            learning_rate: {
                polynomial_decay_learning_rate {
                    initial_learning_rate: 0.001
                    decay_steps: 20000
                    power: 0.9
                }
            }
            momentum_optimizer_value: 0.9
        }
    }
    preprocessor_step {
        random_hue {
            max_delta: 0.15
        }
    }
    preprocessor_step {
        random_sat {
            min_scale: 0.95
            max_scale: 1.05
        }
    }
    preprocessor_step {
        random_image_scale {
            min_scale_ratio: 0.5
            max_scale_ratio: 2.0
        }
    }
    preprocessor_step {
        random_image_crop {
            crop_height: 713
            crop_width: 713
            images_channel_dim: 3
            labels_channel_dim: 1
        }
    }
    preprocessor_step {
        random_horizontal_flip {}
    }
    preprocessor_step {
        salt_and_pepper {
            corrupt_ratio: 0.0005
            kern_size: 10
        }
    }
}

eval_config: {
    eval_input_type {
        padded_eval_input {
            height: 1025
            width: 2049
        }
    }
    flipped: true
}
